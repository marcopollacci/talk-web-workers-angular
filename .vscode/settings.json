{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "workbench.editor.tabSizing": "shrink",
  "git.suggestSmartCommit": false,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.smartCommitChanges": "tracked",
  "gitlens.views.remotes.branches.layout": "tree",
  "eslint.lintTask.enable": true,
  "conventionalCommits.scopes": ["main", "intro", "web-worker-intro", "cover", "thanks", "web-worker-deep"],
  "files.eol": "\n",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css, scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#e8e5f2",
    "activityBar.background": "#e8e5f2",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#bf8e80",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#e8e5f2",
    "statusBar.background": "#cac4e0",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#aca3ce",
    "statusBarItem.remoteBackground": "#cac4e0",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#cac4e0",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#cac4e099",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#cac4e0"
}
