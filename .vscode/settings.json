{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "workbench.editor.tabSizing": "shrink",
  "git.suggestSmartCommit": false,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.smartCommitChanges": "tracked",
  "gitlens.views.remotes.branches.layout": "tree",
  "eslint.lintTask.enable": true,
  "conventionalCommits.scopes": ["main", "intro"],
  "files.eol": "\n",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css, scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#4e97e0",
    "activityBar.background": "#4e97e0",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f5c9df",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#4e97e0",
    "statusBar.background": "#267ed5",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#4e97e0",
    "statusBarItem.remoteBackground": "#267ed5",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#267ed5",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#267ed599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#267ED5"
}
